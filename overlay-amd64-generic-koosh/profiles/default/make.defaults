# Initial value just for style purposes.
USE=""

# Activate the serial port.
USE="${USE} tty_console_ttyS0"
TTY_CONSOLE="ttyS0"
USE="${USE} pcserial"

# Enable kexec file
USE="${USE} kexec_file"

# Enable igb driver
USE="${USE} igb"

# Enable igc driver
USE="${USE} igc"

# Enable aquantia driver
USE="${USE} aquantia"
